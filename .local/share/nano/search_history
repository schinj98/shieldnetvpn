        Process publicKeyProcess = Runtime.getRuntime().exec("wg pubkey");        publicKeyProcess.getOutputStream().write(privateKey.getBytes());        publicKeyProcess.getOutputStream().close();        publicKeyProcess.waitFor();        String publicKey = new String(publicKeyProcess.getInputStream().readAll>        return new String[]{privateKey, publicKey};    }    public File generateConfigFile(String privateKey, String serverPublicKey, S>        File configFile = new File("user-vpn.conf");        try (BufferedWriter writer = new BufferedWriter(new FileWriter(configFi>            writer.write("[Interface]\n");            writer.write("PrivateKey = " + privateKey + "\n");            writer.write("Address = 10.0.0.2/24\n"); // User IP
public File generateConfigFile(String userPrivateKey, String serverPublicKe>        File configFile = File.createTempFile("wg-client-", ".conf");        try (BufferedWriter writer = new BufferedWriter(new FileWriter(configFi>            writer.write("[Interface]\n");            writer.write("PrivateKey = " + userPrivateKey + "\n");            writer.write("Address = 10.0.0.2/24\n");            writer.write("DNS = 8.8.8.8\n");            writer.write("\n");            writer.write("[Peer]\n");            writer.write("PublicKey = " + serverPublicKey + "\n");            writer.write("Endpoint = " + serverIp + ":51820\n");            writer.write("AllowedIPs = " + allowedIps + "\n");            writer.write("PersistentKeepalive = 25\n");        }        return configFile;    }

s
public File generateConfigFile(String userPrivateKey, String serverPublicKey, String serverIp, String allowedIps, long userId) throws IOException {    File configFile = File.createTempFile("wg-client-", ".conf");    String clientIp = "10.0.0." + (userId + 1) + "/24"; // Dynamic IP based on user ID    try (BufferedWriter writer = new BufferedWriter(new FileWriter(configFile))) {        writer.write("[Interface]\n");        writer.write("PrivateKey = " + userPrivateKey + "\n");        writer.write("Address = " + clientIp + "\n");        writer.write("DNS = 8.8.8.8\n");        writer.write("\n");        writer.write("[Peer]\n");        writer.write("PublicKey = " + serverPublicKey + "\n");        writer.write("Endpoint = " + serverIp + ":51820\n");        writer.write("AllowedIPs = " + allowedIps + "\n");        writer.write("PersistentKeepalive = 25\n");    }    return configFile;}


